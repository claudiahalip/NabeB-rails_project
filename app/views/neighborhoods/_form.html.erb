<%=form_for @neighborhood do |f|%>
    <%=f.label :name%><br>
    <%=f.text_field :name%><br>

    <%=f.label :city%><br>
    <%=f.text_field :city%><br>

    <%=f.label :state%><br>
    <%=f.text_field :state%><br>

    <%=f.label :zipcode%><br>
    <%=f.text_field :zipcode%><br>

    <%=f.submit %><br>
  
<%end%>