<h1>New</h1>

<%=form_for :business do |f| %>
  <%=f.label :business_name%><br>
  <%=f.text_field :name%><br>

  <%=f.label :description%><br>
  <%=f.text_area :description%><br>

  <%=f.label :website%><br>
  <%=f.text_field :website%><br>

  <%=f.label :phone_number%><br>
  <%=f.text_field :phone_number%><br>

  <%-f.submit%>

<%end %>